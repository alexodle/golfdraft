<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Pool Party</title>
    <link rel="stylesheet" type="text/css" href="{{cdnUrl}}/dist/bundle.css">

    <script>
      // BOOTSTRAP DATA
      //
      var sd = window.golfDraftSeed = {};
      sd.CDN_URL = '{{cdnUrl}}';

      sd.golfers = {{{golfers}}};
      sd.users = {{{users}}};
      sd.draft = {{{draft}}};
      sd.tourneyStandings = {{{tourneyStandings}}};
      sd.tourney = {{{tourney}}};
      sd.appState = {{{appState}}};
      sd.user = {{{or user "null"}}};
      sd.tourneyName = "{{{tourneyName}}}";
    </script>
  </head>
  <body>
    <section id="golfdraftapp" />
    <script src="{{cdnUrl}}/dist/bundle.js"></script>
  </body>
</html>
